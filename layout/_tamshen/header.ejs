<header class="w3-white header">
    
        <div class="menu">
            <div class="bar">
                <div class="w3-content tsw">
                    <div class="logo"><a href="/"><img src="<%= theme.logo %>" height="50px;" width="50px"></a></div>
                    <div class="menubtn" onclick="menu_open()"><svg t="1554204421219" class="icon"
                            style="width:1.8em;height:1.8em" viewBox="0 0 1024 1024" version="1.1"
                            xmlns="http://www.w3.org/2000/svg" p-id="2695">
                            <path
                                d="M131.754667 202.069333m34.133333 0l682.666667 0q34.133333 0 34.133333 34.133334l0 0q0 34.133333-34.133333 34.133333l-682.666667 0q-34.133333 0-34.133333-34.133333l0 0q0-34.133333 34.133333-34.133334Z"
                                fill="#282828" p-id="2696"></path>
                            <path
                                d="M131.754667 483.669333m34.133333 0l682.666667 0q34.133333 0 34.133333 34.133334l0 0q0 34.133333-34.133333 34.133333l-682.666667 0q-34.133333 0-34.133333-34.133333l0 0q0-34.133333 34.133333-34.133334Z"
                                fill="#282828" p-id="2697"></path>
                            <path
                                d="M131.754667 765.269333m34.133333 0l682.666667 0q34.133333 0 34.133333 34.133334l0 0q0 34.133333-34.133333 34.133333l-682.666667 0q-34.133333 0-34.133333-34.133333l0 0q0-34.133333 34.133333-34.133334Z"
                                fill="#282828" p-id="2698"></path>
                        </svg></div>
                    <div class="menubtnoff" onclick="menu_close()"><svg t="1554205937051" class="icon"
                            style="width:1.8em;height:1.8em" viewBox="0 0 1024 1024" version="1.1"
                            xmlns="http://www.w3.org/2000/svg" p-id="3337">
                            <path
                                d="M890.88 921.6c-7.68 0-15.36-2.56-23.04-10.24L112.64 156.16c-12.8-12.8-12.8-33.28 0-43.52s33.28-12.8 43.52 0l755.2 755.2c12.8 12.8 12.8 33.28 0 43.52-5.12 7.68-12.8 10.24-20.48 10.24"
                                fill="#282828" p-id="3338"></path>
                            <path
                                d="M133.12 921.6c-15.36 0-30.72-12.8-30.72-30.72 0-7.68 2.56-15.36 10.24-23.04L867.84 112.64c12.8-12.8 33.28-10.24 43.52 2.56 10.24 12.8 10.24 30.72 0 43.52L156.16 911.36c-5.12 7.68-12.8 10.24-23.04 10.24"
                                fill="#282828" p-id="3339"></path>
                        </svg></div>
                    <div class="link menu_link">
                        <% Object.keys(theme.menu).forEach(function(key) { %>
                            <% if (url_for(theme.menu[key]) == '/' + path.replace('/index.html', '') || (url_for(theme.menu[key]) == '/' && is_home())) { %>
                            <a class="header-link" href="<%= theme.menu[key] %>" title="<%= key %>">
                                <%= key %></a>
                            <% } else { %>
                            <a class="header-link" href="<%= theme.menu[key] %>" title="<%= key %>">
                                <%= key %></a>
                            <% } %>
                            <% }); %>
                            <% theme.menu2.forEach(muen2 => { %>
                                <div class="w3-dropdown-hover">
                                    <span class="header-link"><%= muen2.title %><span class="header-link-icon"></span></span>
                                    <div class="w3-dropdown-content header-link-pan">
                                        <% Object.keys(muen2.list).forEach(function(key) { %>
                                        <a href="<%= muen2.list[key] %>" title="<%= key %>"><%= key %></a>
                                        <% }); %>
                                    </div>
                                </div>
                                <% }); %>
                    </div>
                </div>
            </div>
        </div>

    </div>
</header>

<div class="w3-content tsw w3-p-b-16" id="barba-wrapper">
    <div class="w3-row barba-container animate-main">